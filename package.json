{
  "name": "api-scaffold",
  "version": "1.0.0",
  "main": "dist/server.js",
  "author": "Joao Paulo Furtado <joaopaulofurtado@live.com>",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "start": "node -r tsconfig-paths/register dist/server.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "verify": "yarn typecheck && yarn lint && yarn test --run",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@keyv/redis": "^4.5.0",
    "@prisma/client": "^6.11.1",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cache-manager": "^7.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonrepair": "^3.12.0",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.3.4",
    "morgan": "^1.10.0",
    "prisma": "^6.11.1",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.6.0",
    "routing-controllers": "^0.11.2",
    "stripe": "^18.3.0",
    "typedi": "^0.10.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.6",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.3",
    "@types/handlebars": "^4.1.0",
    "@types/ioredis": "^5.0.0",
    "@types/ioredis-mock": "^8.2.6",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "ioredis-mock": "^8.9.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
